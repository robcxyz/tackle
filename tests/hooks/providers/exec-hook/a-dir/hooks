
a_funky<-:
  full:
    type: str
    description: A description
    default: a-default
  rendered_by_default:
    type: str
    default: a-default
    render_by_default: true
  compact: a-default

  exec<-:
    full_out->: "{{full}}"
    compact_out->: "{{compact}}"
    rendered_by_default_out->: "{{rendered_by_default}}"
  args:
    - full
